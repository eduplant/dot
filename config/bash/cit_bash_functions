# Binocular telnet wrapper
function bintel ()
{
	ssh -t binocular "telnet $@"
}

# Binocular ping wrapper
function binping ()
{
	ssh -t binocular "ping $@"
}

# Binocular dig wrapper
function bindig ()
{
	ssh -t binocular "dig $@"
}

# Lab VPN wrapper
function labvpn ()
{
	sudo openconnect vdn-lab-asa.caltech.edu
}
